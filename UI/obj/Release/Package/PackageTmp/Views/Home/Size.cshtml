<!DOCTYPE html>
<html>
<head>
    <meta name="viewport" content="width=device-width" />
    <title>Size</title>
    <link href="~/Scripts/assets/css/amazeui.css" rel="stylesheet" />
    <script src="~/Scripts/assets/js/jquery.min.js"></script>
    <script src="~/Scripts/assets/js/amazeui.js"></script>
    <script src="~/Scripts/js/modal.js"></script>
    <script>
        $(function () {
            $("i").click(function () {
                var size = $(this);
                if (size.attr("class").indexOf("left") > 0) {
                    size.removeClass("am-icon-chevron-circle-left").addClass("am-icon-chevron-circle-right");
                    window.top.document.getElementById("frame1").cols = "0,25,*,6";
                } else {
                    size.removeClass("am-icon-chevron-circle-right").addClass("am-icon-chevron-circle-left");
                    window.top.document.getElementById("frame1").cols = "170,25,*,6";
                }
            });
        });
    </script>
</head>
<body>
    <div style ="height:600px;width:25px; background-color:white;line-height:100%;text-align:center;padding-top:255px;border-left:1px solid #e4e1e1;">
        <i class="am-icon-chevron-circle-left am-close am-close-spin" style="margin-top: 45%; color: #0094ff; cursor: pointer;"></i>
    </div>
</body>
</html>
